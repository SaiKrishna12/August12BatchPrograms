public class CreatePromotion {
	
	@Test
	public void testPromotion() throws InterruptedException{
		User user = new User();
		user.setUp();
		user.userLogin();
		user.promo();
		
		
		Thread.sleep(10000);
		user.tearDown();
	}
	
	

}
